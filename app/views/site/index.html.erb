<h1 class="font-bold"> Apple counter </h1>

<div>
  <div id="count">
  <%= @count %> Apples
  </div>
  <div class="flex gap-5 mt-5">
    <%= button_to "Add", add_path, class:"p-2 bg-blue-500 text-white rounded-lg"%>
    <%= button_to "Remove", remove_path, class:"p-2 bg-red-500 text-white rounded-lg"%>

  </div>
  <div id="logs" class="mt-5">
  This is the log
  </div>
</div>